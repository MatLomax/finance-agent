{
  "name": "finance-agent",
  "version": "1.11.0",
  "description": "Ultra-lightweight vanilla JavaScript finance agent",
  "type": "module",
  "main": "src/main.js",
  "scripts": {
    "build": "node build-production.js",
    "build:check": "npm run build && size-limit",
    "build:dev": "mkdir -p dist/styles && cp -r src/styles/* dist/styles/ && cp src/main.js dist/ && echo 'Dev build complete - CSS modularized, JS copied'",
    "test": "node --test src/**/*.test.js src/**/**/*.test.js",
    "test:watch": "node --test --watch src/**/*.test.js src/**/**/*.test.js",
    "test:coverage": "node --test --experimental-test-coverage src/**/*.test.js src/**/**/*.test.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "clean": "npm run knip:fix && npm run lint:fix",
    "check": "./scripts/check.sh",
    "check:fast": "./scripts/check.sh --fast",
    "precommit": "./scripts/check.sh",
    "commit": "echo 'ERROR: Use ./scripts/auto-commit.sh \"detailed commit message\" instead of npm run commit' && exit 1",
    "release": "./scripts/auto-release.sh",
    "ship": "echo 'ERROR: AI agent must use ./scripts/auto-commit.sh \"detailed message\" followed by npm run release' && exit 1"
  },
  "dependencies": {
    "date-fns": "^2.29.3",
    "lodash-es": "^4.17.21",
    "uplot": "^1.6.32"
  },
  "devDependencies": {
    "@size-limit/esbuild": "^11.2.0",
    "@size-limit/file": "^11.2.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.0.0",
    "esbuild": "^0.19.0",
    "eslint": "^8.50.0",
    "jsdom": "^26.1.0",
    "knip": "^5.63.0",
    "size-limit": "^11.2.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "finance",
    "vanilla-javascript",
    "lightweight",
    "esbuild"
  ],
  "author": "",
  "license": "MIT"
}
